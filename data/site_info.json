{
    "jira-inspirato":{
        "site":"https://inspirato.atlassian.net/login.jsp",
        "user_name_element":
            {
                "by":"id",
                "name":"username"
            },
        "password_element":
            {
                "by":"id",
                "name":"password"
            },
        "check_login_element":
            {
                "by":"id",
                "name":"header-details-user-fullname",
                "get_attribute":"data-username"
            },
        "additional_login_action": "None",
        "logout_actions":
            {
                "by":["id","id"],
                "name":["header-details-user-fullname","log_out"],
                "action":["click","click"]
            }
    },
    "stash":{
        "site":"http://stash.dev-charter.net/stash/login",
        "user_name_element":
            {
                "by":"id",
                "name":"j_username"
            },
        "password_element":
            {
                "by":"id",
                "name":"j_password"
            },
        "check_login_element":
            {
                "by":"id",
                "name":"current-user",
                "get_attribute":"data-username"
            },
        "additional_login_action": "None",
        "logout_actions":
            {
                "by":["id","class name"],
                "name":["current-user","logout-link"],
                "action":["click","click"]
            }
    }
}